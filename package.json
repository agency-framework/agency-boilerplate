{
  "name": "agency-boilerplate",
  "version": "3.0.0",
  "description": "agency - frontend boilerplate",
  "keywords": [
    "boilerplate"
  ],
  "homepage": "https://github.com/StephanGerbeth/agency-boilerplate#readme",
  "bugs": {
    "url": "https://github.com/StephanGerbeth/agency-boilerplate/issues"
  },
  "license": "MIT",
  "author": "Stephan Gerbeth <stephan.gerbeth@gmail.com>",
  "contributors": [{
    "name": "Stephan Gerbeth",
    "email": "stephan.gerbeth@grabarzundpartner.de"
  }, {
    "name": "Thorn Walli",
    "email": "lammpee@gmail.com; thorn.walli@grabarzundpartner.de"
  }],
  "main": "none",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StephanGerbeth/agency-boilerplate.git"
  },
  "scripts": {
    "heroku": "node node_modules/gulp/bin/gulp run --serverConfig=./env/config/local.json --configTasks=./env/config/tasks.json --env=production --server=agency-server",
    "build": "node node_modules/gulp/bin/gulp build --serverConfig=./env/config/local.json --configTasks=./env/config/tasks.json --env=build",
    "export": "node node_modules/gulp/bin/gulp export --serverConfig=./env/config/local.json --configTasks=./env/config/tasks.json --env=export",
    "dev": "node node_modules/gulp/bin/gulp run --serverConfig=./env/config/local.json --configTasks=./env/config/tasks.json --env=development --server=agency-server",
    "dev-ip": "node node_modules/gulp/bin/gulp run --serverConfig=./env/config/local.json --configTasks=./env/config/tasks.json --env=development --server=agency-server --ip=true",
    "prod": "node node_modules/gulp/bin/gulp run --serverConfig=./env/config/local.json --configTasks=./env/config/tasks.json --env=production --server=agency-server",
    "server": "node node_modules/gulp/bin/gulp default --serverConfig=./env/config/local.json --env=development --server=agency-server",
    "test": "npm run build",
    "template": "yo agency-boilerplate:hbs-template",
    "create-package": "yo agency-boilerplate:package"
  },
  "dependencies": {
    "agency-environment": "../agency-environment",
    "agency-pkg-base": "../packages/commons/agency-pkg-base",
    "agency-pkg-documentation": "../packages/commons/agency-pkg-documentation",
    "agency-pkg-element-headline": "../packages/elements/agency-pkg-element-headline",
    "agency-pkg-element-link": "../packages/elements/agency-pkg-element-link",
    "agency-pkg-element-modal": "../packages/elements/agency-pkg-element-modal",
    "agency-pkg-element-picture": "../packages/elements/agency-pkg-element-picture",
    "agency-pkg-element-browser-notes": "../packages/elements/agency-pkg-element-browser-notes",
    "agency-pkg-polyfills": "../packages/commons/agency-pkg-polyfills",
    "agency-pkg-service-animation-frame": "../packages/services/agency-pkg-service-animation-frame",
    "agency-pkg-service-history": "../packages/services/agency-pkg-service-history",
    "agency-pkg-service-metrics": "../packages/services/agency-pkg-service-metrics",
    "agency-pkg-service-parser": "../packages/services/agency-pkg-service-parser",
    "agency-pkg-service-viewport": "../packages/services/agency-pkg-service-viewport",
    "agency-pkg-utils": "../packages/commons/agency-pkg-utils",
    "agency-server": "git+https://git@github.com/StephanGerbeth/agency-server.git",
    "color": "^0.11.1",
    "fastdom": "^1.0.1",
    "fastdom-sequencer": "^1.0.3",
    "gsap": "^1.18.4",
    "modernizr": "^3.0.0",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "fb": "^1.0.2",
    "htmlescape": "^1.1.0",
    "jshint": "^2.8.0",
    "minimist": "^1.2.0"
  }
}
