{
  "name": "agency-boilerplate",
  "version": "3.0.0",
  "description": "agency - frontend boilerplate",
  "keywords": [
    "boilerplate"
  ],
  "homepage": "https://github.com/agency-framework/agency-boilerplate#readme",
  "bugs": {
    "url": "https://github.com/agency-framework/agency-boilerplate/issues"
  },
  "license": "MIT",
  "author": "Stephan Gerbeth <stephan.gerbeth@gmail.com>",
  "contributors": [{
    "name": "Stephan Gerbeth",
    "email": "stephan.gerbeth@grabarzundpartner.de"
  }, {
    "name": "Thorn Walli",
    "email": "lammpee@gmail.com; thorn.walli@grabarzundpartner.de"
  }],
  "main": "none",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agency-framework/agency-boilerplate.git"
  },
  "scripts": {
    "heroku": "node node_modules/gulp/bin/gulp run --serverConfig=./env/config/local.json --configTasks=./env/config/tasks.json --env=production --server=agency-server",
    "build": "node node_modules/gulp/bin/gulp build --serverConfig=./env/config/local.json --configTasks=./env/config/tasks.json --env=build",
    "export": "node node_modules/gulp/bin/gulp export --serverConfig=./env/config/local.json --configTasks=./env/config/tasks.json --env=export",
    "dev": "node node_modules/gulp/bin/gulp run --serverConfig=./env/config/local.json --configTasks=./env/config/tasks.json --env=development --server=agency-server",
    "dev-ip": "node node_modules/gulp/bin/gulp run --serverConfig=./env/config/local.json --configTasks=./env/config/tasks.json --env=development --server=agency-server --ip=true",
    "prod": "node node_modules/gulp/bin/gulp run --serverConfig=./env/config/local.json --configTasks=./env/config/tasks.json --env=production --server=agency-server",
    "server": "node node_modules/gulp/bin/gulp default --serverConfig=./env/config/local.json --env=development --server=agency-server",
    "test": "npm run build",
    "template": "yo agency-boilerplate:hbs-template",
    "create-package": "yo agency-boilerplate:package"
  },
  "dependencies": {
    "agency-environment": "git+https://git@github.com/agency-framework/agency-environment.git",
    "agency-pkg-base": "git+https://git@github.com/agency-framework/agency-pkg-base.git",
    "agency-pkg-documentation": "git+https://git@github.com/agency-framework/agency-pkg-documentation.git",
    "agency-pkg-element-headline": "git+https://git@github.com/agency-framework/agency-pkg-element-headline.git",
    "agency-pkg-element-link": "git+https://git@github.com/agency-framework/agency-pkg-element-link.git",
    "agency-pkg-element-modal": "git+https://git@github.com/agency-framework/agency-pkg-element-modal.git",
    "agency-pkg-element-picture": "git+https://git@github.com/agency-framework/agency-pkg-element-picture.git",
    "agency-pkg-element-browser-notes": "git+https://git@github.com/agency-framework/elements/agency-pkg-element-browser-notes.git",
    "agency-pkg-polyfills": "git+https://git@github.com/agency-framework/agency-pkg-polyfills.git",
    "agency-pkg-service-animation-frame": "git+https://git@github.com/agency-framework/agency-pkg-service-animation-frame.git",
    "agency-pkg-service-history": "git+https://git@github.com/agency-framework/agency-pkg-service-history.git",
    "agency-pkg-service-metrics": "git+https://git@github.com/agency-framework/agency-pkg-service-dev-tools.git",
    "agency-pkg-service-parser": "git+https://git@github.com/agency-framework/agency-pkg-service-parser.git",
    "agency-pkg-service-viewport": "git+https://git@github.com/agency-framework/agency-pkg-service-viewport.git",
    "agency-pkg-utils": "git+https://git@github.com/agency-framework/agency-pkg-utils.git",
    "agency-server": "git+https://git@github.com/agency-framework/agency-server.git",
    "color": "^0.11.1",
    "fastdom": "^1.0.1",
    "fastdom-sequencer": "^1.0.3",
    "gsap": "^1.18.4",
    "modernizr": "^3.0.0",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "fb": "^1.0.2",
    "htmlescape": "^1.1.0",
    "jshint": "^2.8.0",
    "minimist": "^1.2.0"
  }
}
